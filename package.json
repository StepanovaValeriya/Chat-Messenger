{
  "name": "chat-messenger",
  "version": "1.0.0",
  "description": "---",
  "scripts": {
    "dev": "webpack serve --stats-error-details",
    "build": "webpack --mode=production --stats-error-details ",
    "start": "npm run build && node ./server.js",
    "test": "jest --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx,ts}\"",
    "stylelint": "stylelint \"src/**/*.{css,.scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,.scss}\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StepanovaValeriya/middle.messenger.praktikum.yandex.git"
  },
  "alias": {
    "core": "./src/core",
    "components": "./src/components",
    "pages": "./src/pages",
    "utils": "./src/utils",
    "api": "./src/api",
    "helpers": "./src/helpers",
    "services": "./src/services"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StepanovaValeriya/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/StepanovaValeriya/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.2",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "eslint": "8.24.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "mini-css-extract-plugin": "2.6.1",
    "msw": "0.48.1",
    "postcss-preset-env": "6.7.0",
    "sass": "1.56.1",
    "sass-loader": "13.1.0",
    "stylelint": "14.13.0",
    "stylelint-scss": "4.3.0",
    "ts-loader": "9.4.1",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "^4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "dotenv": "16.0.3",
    "express": "^4.18.1",
    "handlebars": "^4.7.7",
    "nanoid": "3.3.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^core(.*)$": "<rootDir>/src/core$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^services(.*)$": "<rootDir>/src/services$1",
      "^API(.*)$": "<rootDir>/src/api$1",
      "^store(.*)$": "<rootDir>/src/store$1",
      "^pages(.*)$": "<rootDir>/src/pages$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.ts"
    ]
  }
}
